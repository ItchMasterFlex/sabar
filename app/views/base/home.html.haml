  /[if IE 6]
    = javascript_include_tag "libs/modernizr-2.6.2.min"

  %body
    / SLIDER ###############################################
    %section#header
      .sequence-theme
        #sequence
          %ul.sequence-canvas
            / SEQUENCE 1
            %li.animate-in
              %h2.title A Beautiful Clean Template
              %h3.subtitle The Mountainhead One Page Template is a beautiful, minimal and easy to edit template. This Template Would be perfect for  Creative Agency or a Landing Page.
              %img.model{:alt => "Exemple 1", :src => "img/model1.png"}
    / END SLIDER ###############################################
    / SECTION OUR METHOD ###############################################
    %section#method
      / TITLE
      .row.top-tittle
        .twelve.columns.centered
          %h2 Our Method
          %p.lead Little things that make all the diference
      / ROW
      .row
        .four.columns
          %img{:alt => "Exemple", "data-original" => "img/brain.jpg", :src => "img/brain.jpg"}
            %p Bacon ipsum dolor sit amet ribeye ham kielbasa, pork ball tip jowl pancetta kevin andouille capicola pastrami frankfurter short ribs tenderloin pork chop.
        .four.columns
          %img{:alt => "Exemple", "data-original" => "img/build.jpg", :src => "img/build.jpg"}
            %p Bacon ipsum dolor sit amet ribeye ham kielbasa, pork ball tip jowl pancetta kevin andouille capicola pastrami frankfurter short ribs tenderloin pork chop.
        .four.columns
          %img{:alt => "Exemple", "data-original" => "img/love.jpg", :src => "img/love.jpg"}
            %p Bacon ipsum dolor sit amet ribeye ham kielbasa, pork ball tip jowl pancetta kevin andouille capicola pastrami frankfurter short ribs tenderloin pork chop.
    / END SECTION OUR METHOD ###############################################
    / SECTION PROJECTS ###############################################
    %section#projects
      / TITLE
      .row.top-tittle
        .twelve.columns.centered
          %h2 Projects
          %p.lead Awesome and older works that we build
      / ROW
      #projects_grid.row
        / DATA FILER
        %ul.items.three_up.tiles.centered
          / PROJECTS 1
          %li.item.print
            %a.lightbox{:href => "img/other/2bigf69e.png?lightbox[width]=50p&amp;lightbox[height]=50p"}
              %img{:alt => "", "data-original" => "img/other/2big.png", :src => "img/other/2big.png"}/
              %div
                %h5
                  DAMNED
                  %span Lorem Ipsum
          / PROJECTS 2
          %li.item.print
            %a.lightbox{:href => "img/other/4bigf69e.png?lightbox[width]=50p&amp;lightbox[height]=50p"}
              %img{:alt => "", "data-original" => "img/other/4big.png", :src => "img/other/4big.png"}/
              %div
                %h5
                  MOUNTAIN
                  %span 2012
          / PROJECTS 3
          %li.item.webdesign
            %a.lightbox{:href => "img/other/1bigf69e.png?lightbox[width]=50p&amp;lightbox[height]=50p"}
              %img{:alt => "", "data-original" => "img/other/1big.png", :src => "img/other/1big.png"}/
              %div
                %h5
                  i-Risk
                  %span Webdesign, Identity
